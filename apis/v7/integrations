---
openapi: 3.0.2
info:
  title: Integration_APIs
  version: "3.0"
  description: These are the API's related to Integration framework
  contact:
    url: https://www.opsramp.com/support/
    email: sheshachalam.ratnala@opsramp.com
  license:
    name: GNU GPLv3
    url: https://www.gnu.org/licenses/gpl.txt
servers:
- url: https://<base_url>/api/v2/tenants/<client_id>
  description: This is the base URL
paths:
  /integrations/install/{intgID}:
    summary: Install the selected integation from the given category
    servers:
    - url: https://<base_url>/api/v2/tenants/<client_id>
      description: ""
    post:
      requestBody:
        description: "{\n\t\"displayName\" : \"API Testing\",\n\t\"category\" : \"\
          Monitoring\",\n\t\"alertSource\" : {\n\t\t\"id\" : \"30\"\n\t}\n}"
        content:
          application/json:
            schema:
              $ref: '#/components/schemas/integration_container'
            examples:
              Monitoring-Type:
                value:
                  displayName: API Testing
                  category: Monitoring
                  alertSource:
                    id: "30"
              Install custom integration with only authentication details:
                value:
                  displayName: API Testing
                  category: Custom
                  inboundConfig:
                    authentication:
                      authType: OAUTH2
                      role:
                        uniqueId: ROLE-4718b729-50cc-6634-42c4-ecd09558389d
        required: true
      servers:
      - url: https://<base_url>/api/v2/tenants/<client_id>
        description: ""
      tags:
      - Integration APIs
      responses:
        "200":
          content:
            application/json:
              schema:
                $ref: '#/components/schemas/integration_container'
              examples:
                custom_integration:
                  value:
                    id: INTG-6aa0bbec-ef5b-4ff2-b373-5a4f7ab0a385
                    displayName: API Testing
                    integration:
                      id: CUSTOM
                      name: Custom
                    category: Monitoring
                    alertSource:
                      id: 30
                      name: Custom
                      displayName: Custom
                      techUid: CUSTOM
          description: Returns the installed custom integration with id
      operationId: install
      summary: install integration with custom and defaut type
    parameters:
    - examples:
        integrationID:
          value: CUSTOM
      name: intgID
      schema:
        type: string
      in: path
      required: true
  /integrations/available/search/:
    summary: Search OpsRamp provided integrations
    description: Search all the available integrations list
    servers:
    - url: https://<base_url>/api/v2/tenants/<client_id>
      description: ""
    get:
      servers:
      - url: https://<base_url>/api/v2/tenants/<client_id>
        description: ""
      tags:
      - Integration APIs
      parameters:
      - name: pageNo
        description: the queried page no
        schema:
          type: number
        in: query
      - name: pageSize
        description: Size 0f the page
        schema:
          type: number
        in: query
      - name: isDescendingOrder
        description: ""
        schema:
          type: boolean
        in: query
      - name: sortName
        description: sorting list
        schema:
          type: string
        in: query
      - name: queryString
        description: ""
        schema:
          type: string
        in: query
        required: false
      responses:
        "200":
          content:
            application/json:
              schema:
                $ref: '#/components/schemas/integration_list'
              examples:
                available_integrations:
                  value:
                    results:
                    - id: ALERTLOGIC
                      name: Alert Logic
                    - id: DELLCOMPELLENT
                      name: Dell Compellent
                    - id: Email Alerts
                      name: Email Alerts
                    - id: GRAPHITE
                      name: Graphite
                    - id: NAGIOS
                      name: Nagios
                    - id: NEUSTAR
                      name: Neustar
                    - id: NEWRELIC_LEGACY
                      name: NewRelic_Legacy
                    - id: NIMSOFT
                      name: Nimsoft
                    - id: OPSMANAGER
                      name: OpsManager
                    - id: CUSTOM
                      name: Custom
                    totalResults: 14
                    pageNo: 1
                    pageSize: 10
                    totalPages: 2
                    nextPage: true
                    nextPageNo: 2
                    previousPageNo: 0
                    descendingOrder: false
          description: success get al the available list.
      description: Search in the  available list
