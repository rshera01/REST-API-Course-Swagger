components:
  schemas:
    integration_container:
      title: Root Type for integration_container
      description: Data model for integration
      required:
      - integration
      - logo
      type: object
      properties:
        displayName:
          type: string
        integration:
          $ref: '#/components/schemas/integration_name'
          properties:
            id:
              type: string
            name:
              type: string
        category:
          type: string
        alertSource:
          $ref: '#/components/schemas/alertSource'
          properties:
            id:
              format: int32
              type: integer
            name:
              type: string
            displayName:
              type: string
            techUid:
              type: string
        logo:
          $ref: '#/components/schemas/logos'
          description: 'logo for the installed integration '
        parentIntg:
          $ref: '#/components/schemas/key'
          description: Inheritance proeprty for the integration
        direction:
          description: ""
          type: string
        inboundConfig:
          $ref: '#/components/schemas/inboundConfig'
          description: Optional inbound configuration for custom inteegrations
        outboundConfig:
          $ref: '#/components/schemas/outboundConfig'
          description: Optional outbound configurations
        singleSignOnAuthentication:
          $ref: '#/components/schemas/SSO_configs'
          description: ""
        emailconfiguration:
          $ref: '#/components/schemas/emailconfiguration'
          description: Email alerts payload
      example:
        id: INTG-6aa0bbec-ef5b-4ff2-b373-5a4f7ab0a385
        displayName: API Testing
        integration:
          id: CUSTOM
          name: Custom
        category: Monitoring
        alertSource:
          id: 30
          name: Custom
          displayName: Custom
          techUid: CUSTOM
    integration_type:
      description: Determines the type of  integration  to install
      required:
      - displayName
      - category
